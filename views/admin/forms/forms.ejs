    <div class="content">
	<div class="page-title">
		<ul class="breadcrumb">
			<li>
				<a href="/admin"><i class="fa fa-home"></i></a>
			</li>
			<li>
				<a href="/admin/forms" class="active">Formulare</a>
			</li>
		</ul>
	</div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/forms/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga formular</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:20%">Nume formular</th>
							<th style="width:20%">Template</th>
							<th style="width:10%">Pas</th>
							<th style="width:10%">Status</th>
							<th style="width:5%">Activ</th>
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle"><%- item.Name %></td>
							<td class="v-align-middle"><span class="muted"><i class="fa fa-download"></i></span></td>
							<td class="v-align-middle"><span class="muted"><%- item.Step %></span></td>
							<td class="v-align-middle"><span class="muted"><%- item.Status ? item.Status : '' %></span></td>
							<td class="v-align-middle"><span class="muted"><%- item.isActive ? 'Da' : 'Nu' %></span></td>
							<td><a href="/admin/forms/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Modifica"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/forms/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>

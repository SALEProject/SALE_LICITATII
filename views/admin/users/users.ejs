    <div class="content">
	<div class="page-title">
		<ul class="breadcrumb">
			<li>
				<a href="/admin"><i class="fa fa-home"></i></a>
			</li>
			<li>
				<a href="/admin/users" class="active">Utilizatori</a>
			</li>
		</ul>
	</div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/users/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga utilizator</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:20%">Username</th>
							<th style="width:10%">Prenume</th>
							<th style="width:10%">Nume</th>
							<th style="width:20%">Email</th>
							<th style="width:20%">Agentie</th>
							<th style="width:10%">Roluri</th>
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(users.length>0) { _.each(users,function(item) { %>
						<tr>
							<td class="v-align-middle bold"><%= item.LoginName %></td>
							<td class="v-align-middle"><span class="muted"><%= item.FirstName %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.LastName %></span></td>
							<td><span class="muted"><%= item.Email %></span></td>
							<td><span><%- item.ID_Agency>0 ? toolsService.getArrayItem(req.session.agencies,item.ID_Agency,'CompanyName') : 'Utilizator sistem' %></span></td>
							<td><span><%- item.UserRole %></span></td>
							<td><a href="/admin/users/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Editeaza"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/users/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>

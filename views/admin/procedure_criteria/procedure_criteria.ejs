    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Criteriu Procedura</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/procedure_criteria/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga Crieriu Procedura</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:20%">Nume</th>
							<th style="width:20%">Descriere</th>
							<th style="width:20%">Sters</th>
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle "><%= req.session.getTranslation(item.Name) %></td>
							<td class="v-align-middle "><%= req.session.getTranslation(item.Description) %></td>
							<td class="v-align-middle "><%= (item.isDeleted ? "Da" : "Nu") %></td>						
							<td><a href="/admin/procedure_criteria/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Modifica"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/procedure_criteria/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>

    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Sedinte active</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/asset_schedules/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga sedinta activ</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th class="v-align-middle" style="width:25%">Nume activ</th>
							<th class="v-align-middle" style="width:25%">Ore deschidere</th>
							<th class="v-align-middle" style="width:25%">Ore inchidere</th>
							<th class="v-align-middle" style="width:5%">Activ</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle"><%= toolsService.getArrayItem(req.session.assets,item.ID_Asset,'Name') %></td>
							<td class="v-align-middle"><%= item.PreOpeningTime %> / <%= item.OpeningTime %></td>
							<td class="v-align-middle"><%= item.PreClosingTime %> / <%= item.ClosingTime %></td>
							<td class="v-align-middle"><%- (item.isActive?'Da':'Nu') %></td>
							<td><a href="/admin/asset_schedules/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Editeaza"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/asset_schedules/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>

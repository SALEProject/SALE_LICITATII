    <div class="content">
	  	<div class="page-title">
			<ul class="breadcrumb">
				<li>
					<a href="/admin"><i class="fa fa-home"></i></a>
				</li>
				<li>
					<a href="/admin/rings" class="active">Ringuri</a>
				</li>
			</ul>
	  	</div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/rings/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga ring</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th class="v-align-middle" style="width:20%">Nume</th>
							<th class="v-align-middle" style="width:20%">Cod</th>
							<th class="v-align-middle" style="width:20%">Activ</th>
							<th class="v-align-middle" style="width:20%">Ore deschidere</th>
							<th class="v-align-middle" style="width:20%">Ore inchidere</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle"><%= req.session.getTranslation(item.Name) %></td>
							<td class="v-align-middle"><%= item.Code %></td>
							<td class="v-align-middle"><%- (item.isActive?'Da':'Nu') %></td>
							<td class="v-align-middle"><%= item.PreOpeningTime %> / <%= item.OpeningTime %></td>
							<td class="v-align-middle"><%= item.PreClosingTime %> / <%= item.ClosingTime %></td>
							<td><a href="/admin/assets?ring=<%= item.ID %>" class="action-icon btn btn-small btn-primary" title="Vezi active pe ring"><i class="fa fa-tasks"></i></a></td>
							<td><a href="/admin/rings/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Editeaza"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/rings/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>

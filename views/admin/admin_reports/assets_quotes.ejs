<div class="content">
	<div class="page-title">
		<i class="icon-custom-left"></i>
		<h3>Raport tranzactii activ <%- req.session.getTranslation(asset.Name) %></h3>
	</div>
	<div class="row-fluid">
		<div class="span12">
			<div class="grid simple">
				<div class="grid-title no-border">
					<%- toolsService.getFlashMessage(req.flash()) %>
				</div>
				<div class="grid-body no-border">
					<table class="table table-hover table-condensed admin-table" id="example-table">
						<thead>
							<tr>
								<th style="width:10%">Activ</th>
								<th style="width:10%">Data cotatiei</th>
								<th style="width:10%">Cotatie</th>
								<th style="width:10%">Cantitate</th>
								<th style="width:10%">Moneda</th>
							</tr>
						</thead>
						<tbody>
							<% if(items.length>0) { _.each(items,function(item) { %>
							<tr>
								<td class="v-align-middle"><span class="muted"><%= item.Asset %></span></td>
								<td class="v-align-middle"><span class="muted"><%= item.Date %></span></td>
								<td class="v-align-middle"><span class="muted"><%= item.Quotation %></span></td>
								<td class="v-align-middle bold"><%= item.Quantity %></td>
								<td class="v-align-middle"><span class="muted"><%= item.Currency %></span></td>
							</tr>
							<% }); } %>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	<div class="form-actions">
		<div class="text-center">
			<a class="btn btn-white btn-cons" href="javascript:history.back()">Inapoi</a>
		</div>
	</div>
</div>

<div class="tile white m-b-10 padding-20">
	<div class="tile-title">
		<h3 class="m-t-0 m-b-10"><%= title %></h4>
		<%- toolsService.getFlashMessage(req.flash()) %>
	</div>
	<div class="tile-body p-t-20">
		<div class="account-form">
			<table class="table table-hover table-condensed admin-table" id="example-table">
				<thead>
					<tr>
						<th class="v-align-middle" style="width:10%">Nume</th>
						<th class="v-align-middle" style="width:10%">Cod</th>
						<th class="v-align-middle" style="width:10%">Ring</th>
						<th class="v-align-middle" style="width:10%">Data</th>
						<th class="v-align-middle" style="width:10%">Activ</th>
						<th class="v-align-middle" style="width:10%">Unitate de masura</th>
						<th class="v-align-middle" style="width:10%">Valuta</th>
						<th class="v-align-middle" style="width:5%">&nbsp;</th>
					</tr>
				</thead>
				<tbody>
					<% if(items.length>0) { 
						_.each(items,function(item) { %>
							<tr>
								<td class="v-align-middle"><%= req.session.getTranslation(item.AssetName) %></td>									
								<td class="v-align-middle"><%= item.AssetCode %></td>
								<td class="v-align-middle"><%= req.session.getTranslation(item.RingName) %></td>
								<td class="v-align-middle"><%= item.StartDate %></td>
								<td class="v-align-middle"><%- (item.AssetActive?'Da':'Nu') %></td>
								<td class="v-align-middle"><%= req.session.getTranslation(item.MeasuringUnit) %></td>
								<td class="v-align-middle"><%= req.session.getTranslation(item.Currency) %></td>
								<td><a href="/account/agency_assets/<%= item.ID %>" class="action-icon btn btn-small btn-primary" title="Vezi activ"><i class="fa fa-edit"></i></a></td>
							</tr>
					<% }); 
					} %>
				</tbody>
			</table>
		</div>
	</div>
</div>

    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Terminale</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/terminals/add" class="btn btn-info"><i class="fa fa-plus-circle"></i>&nbsp;Adauga terminal</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:20%">Cod</th>
							<th style="width:10%">Nume</th>
							<th style="width:10%">Administrator</th>
							<th style="width:20%">Status</th>
							<th style="width:20%">Oras</th>						
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle bold"><%= item.Code %></td>
							<td class="v-align-middle"><span class="muted"><%= item.Name %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.Administrator %></span></td>
							<td class="v-align-middle"><span class="muted"><%= (item.isActive)?"Activ":"Inactiv" %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.City %></span></td>							
							<td><a href="/admin/terminals/edit/<%= item.ID %>" class="action-icon btn btn-small btn-success" title="Editeaza"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/terminals/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-danger"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>
